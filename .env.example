# Frontend Environment Variables
# Copy this file to .env.local and fill in your values

# API URL (leave empty to use proxy configuration)
VITE_API_URL=

# For socket connections (leave empty to use current origin)
VITE_SOCKET_URL=

# Cloudinary Upload Preset (for client-side uploads)
VITE_CLOUDINARY_UPLOAD_PRESET=naattumarket_uploads
VITE_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name

# Base URL
VITE_BASE_URL=http://localhost:5173
